import tkinter as tk
from tkinter import messagebox

class törelmelikkurumbölümyerleştirici:
    def __init__(self, root):
        self.root = root
        self.root.title("törelmelik kurum bölüm yerleştiricisi")
        self.current_soru = None
        self.current_code = None
        self.flowchart = None
        
        # Define flowcharts
        self.flowcharts = {
            "kaynak": self.kaynak_flowchart(),
            "anlatım": self.anlatım_flowchart(),
            "değer": self.değer_flowchart(),
            "gösterimler": self.gösterimler_flowchart(),
            "etkilenmeyenler": self.etkilenmeyenler_flowchart(),
            "uygulayanlar": self.uygulayanlar_flowchart(),
            "uyguraçları": self.uyguraçları_flowchart(),
            "tekrarlayanlar": self.tekrarlayanlar_flowchart(),
            "onaylamayanlar": self.onaylamayanlar_flowchart(),
            "onaylayanlar": self.onaylayanlar_flowchart()
        }
        
        # Create main frame
        self.main_frame = tk.Frame(self.root)
        self.main_frame.pack(padx=20, pady=20)
        
        # Create selection screen
        self.create_selection_screen()
    
    def create_selection_screen(self):
        # Clear main frame
        for widget in self.main_frame.winfo_children():
            widget.destroy()
            
        tk.Label(self.main_frame, text="törelmelik yöntem seçin", font=("Arial", 14)).pack(pady=10)
        
        for flowchart_name in self.flowcharts.keys():
            tk.Button(self.main_frame, text=flowchart_name, 
                     command=lambda name=flowchart_name: self.start_flowchart(name)).pack(pady=5)
    
    def start_flowchart(self, flowchart_name):
        self.flowchart = flowchart_name
        self.current_code = None
        self.current_soru = list(self.flowcharts[flowchart_name].keys())[0]
        self.show_soru()
    
    def show_soru(self):
        # Clear main frame
        for widget in self.main_frame.winfo_children():
            widget.destroy()
            
        if self.current_soru is None:
            self.create_selection_screen()
            return
            
        soru_data = self.flowcharts[self.flowchart][self.current_soru]
        soru_text = soru_data["text"]
        seçenekler = soru_data["seçenekler"]
        
        tk.Label(self.main_frame, text=soru_text, font=("Arial", 14)).pack(pady=10)
        
        # Create buttons for seçenekler
        for option, next_step in seçenekler.items():
            tk.Button(self.main_frame, text=option, 
                     command=lambda opt=option, step=next_step: self.handle_choice(opt, step)).pack(pady=5)
        
        # Back button
        tk.Button(self.main_frame, text="seçimelere geri dön", 
                 command=self.create_selection_screen).pack(pady=10)
    
    def handle_choice(self, option, next_step):
        # Update current code
        if self.current_code is None:
            self.current_code = next_step["code"]
        else:
            self.current_code = next_step["code"]
            
        if next_step["type"] == "soru":
            self.current_soru = next_step["next"]
            self.show_soru()
        else:
            messagebox.showinfo("Result", next_step["result"])
            self.current_soru = None
            self.show_soru()
    
